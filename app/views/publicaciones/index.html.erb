<p id="notice"><%= notice %></p>

<h1>Publicaciones</h1>

<table>
  <thead>
    <tr>
      <th>Foto</th>
      <th>Estado</th>
      <th>Usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @publicaciones.each do |publicacione| %>
      <tr>
        <td><%= publicacione.foto %></td>
        <td><%= publicacione.estado_id %></td>
        <td><%= publicacione.usuario_id %></td>
        <td><%= link_to 'Show', publicacione %></td>
        <td><%= link_to 'Edit', edit_publicacione_path(publicacione) %></td>
        <td><%= link_to 'Destroy', publicacione, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Publicacione', new_publicacione_path %>
